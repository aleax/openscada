		<prm id="StartMod">QTCfg</prm>
		<prm id="CloseToTray">0</prm>
		<prm id="Style"></prm>
		<prm id="Font"></prm>
		<prm id="Palette"></prm>
		<prm id="StyleSheets"></prm>
