msgid "Increment"
msgstr "Inkrement"

msgid ""
"Increment per formula:\n"
"out = if( in > prev ) then prev + (k+)*(in-prev); else prev - (k-)*(prev-in);"
msgstr ""
"Inkrement nach der Formel:\n"
"out = if( in > prev ) then prev + (k+)*(in-prev); else prev - (k-)*(prev-in);"

msgid "Input 6"
msgstr "Eingang 6"

msgid "Divider"
msgstr "Division"

msgid ""
"Divider per formula:\n"
"out = (in1_1*in1_2*in1_3*in1_4*in1_5 + in2_1*in2_2*in2_3*in2_4*in2_5 + "
"in3) /\n"
"      (in4_1*in4_2*in4_3*in4_4*in4_5 + in5_1*in5_2*in5_3*in5_4*in5_5 + in6);"
msgstr ""
"Division nach der Formel:\n"
"out = (in1_1*in1_2*in1_3*in1_4*in1_5 + in2_1*in2_2*in2_3*in2_4*in2_5 + "
"in3) /\n"
"      (in4_1*in4_2*in4_3*in4_4*in4_5 + in5_1*in5_2*in5_3*in5_4*in5_5 + in6);"

