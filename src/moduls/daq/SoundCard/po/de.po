msgid "Sound card"
msgstr "Soundkarte"

msgid "Roman Savochenko"
msgstr "Roman Savotschenko"

msgid "Allow access to sound card data."
msgstr "Zugriff auf Soundkartendata."

msgid "Parameters' table"
msgstr "Tabelle der Parameter"

msgid "Card device"
msgstr "Kartengerät"

msgid "Card sample rate (Hz)"
msgstr "Abtastrate der Karte (Hz)"

msgid "Card sample type"
msgstr "Typ Abtasten der Karte"

msgid "Float 32;Int 32;Int 16"
msgstr "Fliesskomma 32;Ganzzahl 32;Ganzzahl 16"

msgid "Gather task priority"
msgstr "Priorität der Datenerfassung"

msgid "Standard"
msgstr "Standart"

msgid "Channel"
msgstr "Kanal"

msgid "Value"
msgstr "Variable"

#, c-format
msgid "Allowed %d input channels"
msgstr "Verfügbar %d Eingangskanäle"

#, c-format
msgid ""
"Gathering from %d channels. Recieved %.2f MB. Adjusted samplerate %d. Lost "
"frames %g"
msgstr ""
"Erfassung von %d Kanälen. Empfangen %.2f MB. Customized Probenahme %d. "
"Verlorenen Frames %g"

#, c-format
msgid "Selected device '%s' is error or default device no allow."
msgstr ""
"Fehler beim ausgewählten Gerät '%s' oder das Standardgerät ist nicht "
"zulässig."

msgid "No one channel is configured for acquisition."
msgstr "Empfangsskanal ist nicht konfiguriert."

msgid "Sample type set is error."
msgstr "Fehler Bein Einstellen der Abtastung."

msgid "Sound input stream is not stopped!"
msgstr "Datenerfassung Toneingang ist nicht angehalten!"

#, c-format
msgid ""
"CallBack: Lost frames correct: framesPerBuffer=%lu; sRt=%d; t_sz=%lld; err="
"%g; diff=%lld."
msgstr "Rückruffunktion: Korrektur Frame-Verlust: framesPerBuffer=%lu; sRt=%d; t_sz=%lld; err="
"%g; diff=%lld."

#, c-format
msgid "CallBack: Sound counter difference fix: sRt=%d; dTm=%lld."
msgstr "Rückruffunktion: Klingt Zähler Unterschied fix: sRt=%d; dTm=%lld."
