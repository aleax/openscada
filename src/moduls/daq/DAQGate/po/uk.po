msgid "Parameter"
msgstr "Параметр"

#: daq_gate.cpp:960
msgid "State"
msgstr "Стан"

#: daq_gate.cpp:962
msgid "Type"
msgstr "Тип"

#: daq_gate.cpp:964
msgid "Enable"
msgstr "Ввімкнено"

#: daq_gate.cpp:965
msgid "Id"
msgstr ""

#: daq_gate.cpp:966
msgid "Name"
msgstr "Ім'я"

#: daq_gate.cpp:968
msgid "Configuration"
msgstr "Конфігурація"

#: daq_gate.cpp:984
msgid "Remote station configuration"
msgstr "Конфігурація віддаленої станції"

