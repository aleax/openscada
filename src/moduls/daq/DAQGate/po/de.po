msgstr "Parameter"

msgid "State"
msgstr "Zustand"

msgid "Type"
msgstr "Typ"

msgid "Enabled"
msgstr "Aktiviert"

msgid "Identifier"
msgstr "Identifikator"

msgid "Name"
msgstr "Name"

msgid "Configuration"
msgstr "Konfiguration"

