"    if( allFnt.length >= 1 ) this.wFont+='font-family: '+allFnt[0].replace(/"
"_/g,' ')+'; ';\n"
"    if( allFnt.length >= 2 ) this.wFont+='font-size: '+(parseInt(allFnt[1])"
"*Math.min(xSc,ySc)).toFixed(0)+'px; ';\n"
"    if( allFnt.length >= 3 ) this.wFont+='font-weight: '+(parseInt(allFnt"
"[2])?'bold':'normal')+'; ';\n"
"    if( allFnt.length >= 4 ) this.wFont+='font-style: '+(parseInt(allFnt"
"[3])?'italic':'normal')+'; ';\n"
"\n"
"    if( !this.place.firstChild )\n"
"    {\n"
"      this.place.appendChild(document.createElement('table'));\n"
"      this.place.firstChild.wdgLnk = this;\n"
"      this.place.firstChild.isActive = (parseInt(this.attrs['active']) && "
"parseInt(this.attrs['perm'])&SEQ_WR);\n"
