"\t  this.curArch = this.attrs['arch'];\n"
"\t  this.curTmpl = this.attrs['tmpl'];\n"
"\t  this.curLev = this.attrs['lev'];\n"
"\t  var rowEl = document.createElement('tr');\n"
"\t  tblB.appendChild(rowEl);\n"
