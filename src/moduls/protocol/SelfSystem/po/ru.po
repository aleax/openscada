msgid "Self system OpenSCADA protocol"
msgstr "Собственный протокол системы OpenSCADA"

msgid "Roman Savochenko"
msgstr "Роман Савоченко"

msgid "Self OpenSCADA protocol, support generic functions."
msgstr "Собственный протокол OpenSCADA, поддерживает основные функции."

#, c-format
msgid "Station '%s' error: %s!"
msgstr "Станция '%s' ошибка: %s!"

#, c-format
msgid "Station '%s' error: Respond format error!"
msgstr "Ошибка станции '%s': Ответ не корректен!"

#, c-format
msgid "Station respond '%s' error!"
msgstr "Ошибка ответа станции '%s'!"

msgid "Not full respond."
msgstr "Неполный ответ."

msgid "Parameters"
msgstr "Параметры"

msgid "Module options"
msgstr "Опции модуля"

msgid "Life time of auth session(min)"
msgstr "Время жизни сессии (мин)"

msgid "Compression level"
msgstr "Уровень компрессии"

msgid ""
"ZLib compression level:\n"
"  -1  - optimal speed-size;\n"
"  0   - disable;\n"
"  1-9 - direct level."
msgstr ""
"Уровень компрессии ZLib:\n"
"  -1  - оптимальный, скорость-размер;\n"
"  0   - отключен;\n"
"  1-9 - прямой уровень."

msgid "Lower compression border"
msgstr "Нижняя граница компрессии"

msgid "Value in bytes."
msgstr "Значение в байтах."

#, c-format
msgid "Get request: '%s': %d"
msgstr "Запрос Get: '%s': %d"

#, c-format
msgid "Unpack and load request: '%s': %d, time: %f ms."
msgstr "Распаковка и загрузка запроса: '%s': %d, время: %f мс."

#, c-format
msgid "Process request: '%s', time: %f ms."
msgstr "Обработка запроса: '%s', время: %f мс."

#, c-format
msgid "Save respond to stream and pack: '%s': %d, time: %f ms."
msgstr "Сохранение ответа в поток и упаковка: '%s': %d, время: %f мс."
